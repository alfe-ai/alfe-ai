{% if section.settings.text != blank %}
  <div class="announcement">{{ section.settings.text }}</div>
{% endif %}

{% schema %}
{
  "name": "Announcement bar",
  "settings": [
    {
      "type": "text",
      "id": "text",
      "label": "Text",
      "default": "Welcome to Alfe AI subscriptions"
    }
  ]
}
{% endschema %}
