
# Enable account registration and login features (optional, defaults to disabled)
ACCOUNTS_ENABLED=false

# Redirect the Code view to code.alfe.sh (optional, default: disabled)
CODE_ALFE_REDIRECT=false

# OpenAI API key for AI features (optional)
# OPENAI_API_KEY=sk-xxxxxxxx

# Optional default AI model used if DB is empty
# AI_MODEL=openrouter/qwen/qwen3-30b-a3b-instruct-2507

# Stability AI API key for the upscale script (optional)
# STABILITY_API_KEY=sk-xxxxxxxx

# Path to the Printify submission script (optional)
# PRINTIFY_SCRIPT_PATH=/path/to/run.sh
# Path to the Printify price update script (optional)
# PRINTIFY_PRICE_SCRIPT_PATH=/path/to/run_price.sh
# Path to the Printify title fix script (optional)
# PRINTIFY_TITLE_FIX_SCRIPT_PATH=./scripts/printifyTitleFix.js
# Base URL for the ProgramaticPuppet API (optional)
# PROGRAMATIC_PUPPET_API_BASE=https://localhost:3005
# Printify API credentials
# PRINTIFY_API_TOKEN=yourToken
# Alternatively you can use the older PRINTIFY_TOKEN variable
# PRINTIFY_SHOP_ID=yourShopId

# Base URL of your self-hosted Stable Diffusion API (optional)
# STABLE_DIFFUSION_URL=http://localhost:7860

# Path to SSL private key for HTTPS (optional)
# HTTPS_KEY_PATH=/path/to/privkey.pem

# Path to SSL certificate for HTTPS (optional)
# HTTPS_CERT_PATH=/path/to/fullchain.pem

# Port for the Aurora web server (optional)
# AURORA_PORT=3000

# Disable TOTP requirement on login (optional)
# DISABLE_2FA=true

# PostgreSQL connection string for AWS RDS (optional)
# AWS_DB_URL=postgres://user:password@hostname:5432/dbname

# If AWS_DB_URL is not provided, configure these instead. Setting AWS_DB_HOST
# will also enable the RDS integration.
# AWS_DB_HOST=hostname
# AWS_DB_USER=username
# AWS_DB_PASSWORD=password
# AWS_DB_NAME=dbname
# AWS_DB_PORT=5432

# Optional comma-separated list of IP addresses allowed to access the web UI. Requests from localhost are always allowed
# WHITELIST_IP=1.2.3.4,5.6.7.8

# Port for the SQL passthrough server
# SQL_SERVER_PORT=7000

# Path to the SQLite database
# SQLITE_DB_PATH=issues.sqlite

# Enable/disable the ProjectView UI and API (default: false)
AURORA_PROJECTVIEW_ENABLED=false
